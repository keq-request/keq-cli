{
  "type": "object",
  "required": [
    "outdir",
    "modules"
  ],
  "properties": {
    "outdir": {
      "type": "string"
    },
    "strict": {
      "type": "boolean",
      "default": true
    },
    "fileNamingStyle": {
      "type": "string",
      "enum": [
        "camelCase",
        "capitalCase",
        "constantCase",
        "dotCase",
        "headerCase",
        "noCase",
        "paramCase",
        "pascalCase",
        "pathCase",
        "sentenceCase",
        "snakeCase"
      ]
    },
    "request": {
      "type": "string"
    },
    "modules": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    }
  }
}
